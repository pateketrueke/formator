{
  "name": "jsonschema-form-mw",
  "version": "0.1.0",
  "files": [
    "index.js",
    "dist"
  ],
  "scripts": {
    "dev": "tarima watch -d",
    "dist": "tarima -fqe production"
  },
  "author": "Alvaro Cabrera <pateketrueke@gmail.com>",
  "license": "MIT",
  "tarima": {
    "from": "src",
    "output": "dist",
    "rename": [
      "**/src/**:{fullpath/1}"
    ],
    "filter": [
      "!_*",
      "!**/_*",
      "!**/_*/**"
    ],
    "bundle": [
      "**/main.js"
    ],
    "ignoreFiles": [
      ".gitignore"
    ],
    "bundleOptions": {
      "extensions": {
        "js": "es6",
        "css": "less"
      },
      "less": {
        "plugins": [
          "less-plugin-autoprefix"
        ]
      }
    }
  },
  "devDependencies": {
    "buble": "^0.19.3",
    "less": "^2.7.3",
    "less-plugin-autoprefix": "^1.5.1",
    "rollup": "^0.57.1",
    "tarima": "^4.0.7"
  }
}
