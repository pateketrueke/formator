{
  "$attributes": {
    "findAll": ["name", "path", "type", "size"]
  },
  "$schema": {
    "id": "File",
    "properties": {
      "id": {
        "type": "integer",
        "primaryKey": true
      },
      "mtime": {
        "type": "string",
        "format": "datetime"
      },
      "name": {
        "type": "string"
      },
      "path": {
        "type": "string"
      },
      "size": {
        "type": "integer"
      },
      "type": {
        "type": "string"
      }
    }
  }
}
