{
  "$attributes": {
    "findAll": [
      "url",
      "blob",
      "file",
      "title",
      "fileset.name",
      "fileset.path",
      "fileset.size",
      "fileset.mtime",
      "file.name",
      "file.path",
      "file.size",
      "file.mtime"
    ]
  },
  "$uiSchema": {
    "ui:pending": "Saving changes...",
    "ui:fields": ["id", "url", "file", "blob", "title", "fileset"],
    "ui:headers": ["#ID", "Image URL", "File", "Base64 Blob", "Example's title", "Images"],
    "blob": {
      "ui:template": [["span", "{@embed:this}"]],
      "ui:component": "FileUpload",
      "ui:attachment": true
    },
    "fileset": {
      "ui:template": [["span", "{@embed:*.*}"]],
      "ui:component": "FileUpload"
    },
    "file": {
      "ui:template": [["span", "{@embed:this}"]],
      "ui:component": "FileUpload"
    },
    "url": {
      "ui:template": [["span", "{@embed:this}"]],
      "ui:component": "FileUpload"
    }
  },
  "$schema": {
    "id": "Example",
    "properties": {
      "id": {
        "type": "integer",
        "primaryKey": true
      },
      "title": {
        "type": "string"
      },
      "blob": {
        "attachment": true,
        "type": "string"
      },
      "url": {
        "type": "string"
      },
      "file": {
        "$ref": "File"
      },
      "fileset": {
        "type": "array",
        "items": {
          "$ref": "File"
        }
      }
    }
  }
}
