{
  "$attributes": {
    "findAll": [
      "url",
      "blob",
      "file",
      "title",
      "fileset.name",
      "fileset.path",
      "fileset.size",
      "fileset.mtime",
      "file.name",
      "file.path",
      "file.size",
      "file.mtime"
    ]
  },
  "$uiSchema": {
    "ui:add": false,
    "ui:pending": "Saving changes...",
    "ui:fields": ["id", "title", "url", "file", "blob", "fileset"],
    "ui:headers": ["#", "Example", "URL", "File", "Blob", "Images"],
    "blob": {
      "ui:template": [["div", "{@embed:this}"]],
      "ui:component": "FileUpload",
      "ui:attachment": true
    },
    "url": {
      "ui:template": [["div", "{@file:this}"]],
      "ui:component": "FileUpload"
    },
    "fileId": {
      "ui:hidden": true
    },
    "id": {
      "ui:edit": false
    }
  },
  "$schema": {
    "id": "Example",
    "properties": {
      "id": {
        "type": "integer",
        "primaryKey": true
      },
      "title": {
        "type": "string"
      },
      "blob": {
        "attachment": true,
        "type": "string"
      },
      "url": {
        "type": "string"
      },
      "file": {
        "$ref": "File",
        "belongsTo": {
          "through": {
            "model": "Attachment"
          }
        }
      },
      "fileset": {
        "type": "array",
        "items": {
          "$ref": "File",
          "belongsToMany": {
            "through": {
              "model": "Attachment"
            }
          }
        }
      }
    }
  }
}
